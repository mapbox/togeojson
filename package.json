    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@xmldom/xmldom": "^0.8.2",
    "eslint": "^8.14.0",
    "jest": "^28.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
